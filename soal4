#include <iostream>
using namespace std;
int main()
{
    int n,C;
    
    cout<<"jumlah bilangan = ";
    cin>>n;
    
    int A[n];
    
    for (C=1; C<=n; C++){
        cout<<"A["<<C<<"] = ";
        cin>>A[C];
    }
    
    cout<<"Nilai yang anda masukan = "<<endl;
    
    for (C=1; C<=n; C++){
        cout<<A[C]<<endl;
    }
    
    return 0;
}
