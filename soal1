#include <iostream>

using namespace std;

int main()
{
    int A[5]={10,20,30,40,50};

    cout<<"sebelum dilakukan perubahan nilai"<<endl;
    cout<<"A[0] = "<<A[0]<<endl;
    cout<<"A[0] = "<<A[1]<<endl;
    cout<<"A[0] = "<<A[2]<<endl;
    cout<<"A[0] = "<<A[3]<<endl;
    cout<<"A[0] = "<<A[4]<<endl;

    return 0;
}
